    "    def __init__(self, n):\n",
    "        self.arr = [None] * n\n",
    "        self.top1 = -1\n",
    "        self.top2 = n\n",
    "\n",
    "stack = TwoStacks(5)\n",
    "print(stack.arr)  # Output: [None, None, None, None, None]\n",
    "print(stack.top1)  # Output: -1\n",
    "print(stack.top2)  # Output: 5"
